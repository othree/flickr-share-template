{
    "name": "Flickr Share with Template",
    "version": "0.1",
    "manifest_version": 2,
    "icons": {
    	"128": "icons/128x128.png",
    	"48":  "icons/48x48.png",
    	"16":  "icons/16x16.png"
    },
    "browser_action": {
        "default_icon": {
    		"19": "icons/19x19.png",
    		"38": "icons/38x38.png"
    	},
        "default_popup": "popup.html"
    },
    "permissions": ["tabs", "storage"]
}